                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>
                            </VisualStateManager.VisualStateGroups>
                            <Grid x:Name="HorizontalRoot" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" IsHitTestVisible="False">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="Auto"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="Auto"/>
                                </Grid.ColumnDefinitions>
                                <Rectangle RadiusX="5" RadiusY="5" x:Name="HorizontalTrackRect" Grid.ColumnSpan="5" Fill="{ThemeResource ScrollBarTrackFill}" Margin="0" Opacity="0" StrokeThickness="{ThemeResource ScrollBarTrackBorderThemeThickness}" Stroke="{ThemeResource ScrollBarTrackStroke}"/>
                                <RepeatButton x:Name="HorizontalSmallDecrease" AllowFocusOnInteraction="False" Grid.Column="0" Interval="50" IsTabStop="False" MinHeight="{ThemeResource ScrollBarSize}" Margin="0" Opacity="0" Template="{StaticResource HorizontalDecrementTemplate}" VerticalAlignment="Center" Width="{ThemeResource ScrollBarSize}"/>
                                <RepeatButton x:Name="HorizontalLargeDecrease" AllowFocusOnInteraction="False" Grid.Column="1" HorizontalAlignment="Stretch" Interval="50" IsTabStop="False" Opacity="0" Template="{StaticResource RepeatButtonTemplate}" VerticalAlignment="Stretch" Width="0"/>
                                <Thumb x:Name="HorizontalThumb" AutomationProperties.AccessibilityView="Raw" Background="{ThemeResource ScrollBarPanningThumbBackground}" Grid.Column="2" Height="{ThemeResource ScrollBarSize}" MinWidth="{ThemeResource ScrollBarSize}" Opacity="0" RenderTransformOrigin="0.5,1" Template="{StaticResource HorizontalThumbTemplate}" VerticalAlignment="Bottom">
                                    <Thumb.RenderTransform>
                                        <CompositeTransform x:Name="HorizontalThumbTransform" ScaleX="1.0" ScaleY="{ThemeResource SmallScrollThumbScale}" TranslateX="0" TranslateY="{ThemeResource SmallScrollThumbOffset}"/>
                                    </Thumb.RenderTransform>
                                </Thumb>
                                <RepeatButton x:Name="HorizontalLargeIncrease" AllowFocusOnInteraction="False" Grid.Column="3" HorizontalAlignment="Stretch" Interval="50" IsTabStop="False" Opacity="0" Template="{StaticResource RepeatButtonTemplate}" VerticalAlignment="Stretch"/>
                                <RepeatButton x:Name="HorizontalSmallIncrease" AllowFocusOnInteraction="False" Grid.Column="4" Interval="50" IsTabStop="False" MinHeight="{ThemeResource ScrollBarSize}" Margin="0" Opacity="0" Template="{StaticResource HorizontalIncrementTemplate}" VerticalAlignment="Center" Width="{ThemeResource ScrollBarSize}"/>
                            </Grid>
                            <Grid x:Name="HorizontalPanningRoot" MinWidth="24" Opacity="0" Visibility="Collapsed">
                                <Border x:Name="HorizontalPanningThumb" Background="{ThemeResource ScrollBarPanningThumbBackground}" BorderThickness="0" HorizontalAlignment="Left" Height="2" MinWidth="32" Margin="0,2,0,2" VerticalAlignment="Bottom"/>
                            </Grid>
                            <Grid x:Name="VerticalRoot" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" IsHitTestVisible="False" Margin="0,45,0,30">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <!--Fill="{ThemeResource ScrollBarTrackFill}"    {ThemeResource ScrollBarPanningThumbBackground}-->
                                <Rectangle Width="2" x:Name="VerticalTrackRect" Fill="Black" Margin="0" Opacity="1" Grid.RowSpan="5" StrokeThickness="{ThemeResource ScrollBarTrackBorderThemeThickness}" Stroke="{ThemeResource ScrollBarTrackStroke}"/>
                                <RepeatButton x:Name="VerticalSmallDecrease" HorizontalAlignment="Center" Height="{ThemeResource ScrollBarSize}" Interval="50" IsTabStop="False" MinWidth="{ThemeResource ScrollBarSize}" Margin="0" Opacity="0" Grid.Row="0" Template="{StaticResource VerticalDecrementTemplate}"/>
                                <RepeatButton x:Name="VerticalLargeDecrease" AllowFocusOnInteraction="False" HorizontalAlignment="Stretch" Height="0" Interval="50" IsTabStop="False" Opacity="0" Grid.Row="1" Template="{StaticResource RepeatButtonTemplate}" VerticalAlignment="Stretch"/>
                                <Thumb x:Name="VerticalThumb" AutomationProperties.AccessibilityView="Raw" Background="{ThemeResource ScrollBarPanningThumbBackground}" MinHeight="{ThemeResource ScrollBarSize}" Opacity="0" RenderTransformOrigin="1,0.5" Grid.Row="2" Template="{StaticResource VerticalThumbTemplate}" Width="{ThemeResource ScrollBarSize}">
                                    <Thumb.RenderTransform>
                                        <CompositeTransform x:Name="VerticalThumbTransform" ScaleX="{ThemeResource SmallScrollThumbScale}" ScaleY="1.0" TranslateX="{ThemeResource SmallScrollThumbOffset}" TranslateY="0"/>
                                    </Thumb.RenderTransform>
                                </Thumb>
                                <RepeatButton x:Name="VerticalLargeIncrease" AllowFocusOnInteraction="False" HorizontalAlignment="Stretch" Interval="50" IsTabStop="False" Opacity="0" Grid.Row="3" Template="{StaticResource RepeatButtonTemplate}" VerticalAlignment="Stretch"/>
                                <RepeatButton x:Name="VerticalSmallIncrease" HorizontalAlignment="Center" Height="{ThemeResource ScrollBarSize}" Interval="50" IsTabStop="False" MinWidth="{ThemeResource ScrollBarSize}" Margin="0" Opacity="0" Grid.Row="4" Template="{StaticResource VerticalIncrementTemplate}"/>
                            </Grid>
                            <Grid x:Name="VerticalPanningRoot" MinHeight="24" Opacity="0" Visibility="Collapsed">
                                <Border x:Name="VerticalPanningThumb" Background="Black" BorderThickness="0" HorizontalAlignment="Right" MinHeight="32" Margin="2,0,2,0" VerticalAlignment="Top" Width="2"/>
                            </Grid>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="GridViewItemContainerStyle" TargetType="GridViewItem">
            <Setter Property="FontFamily" Value="{ThemeResource ContentControlThemeFontFamily}"/>
            <Setter Property="FontSize" Value="{ThemeResource ControlContentThemeFontSize}"/>
            <Setter Property="Background" Value="{ThemeResource GridViewItemBackground}"/>
            <Setter Property="Foreground" Value="{ThemeResource GridViewItemForeground}"/>
            <Setter Property="TabNavigation" Value="Local"/>
            <Setter Property="IsHoldingEnabled" Value="True"/>
            <Setter Property="HorizontalContentAlignment" Value="Center"/>
            <Setter Property="VerticalContentAlignment" Value="Center"/>
            <Setter Property="Margin" Value="0,0,4,4"/>
            <Setter Property="MinWidth" Value="{ThemeResource GridViewItemMinWidth}"/>
            <Setter Property="MinHeight" Value="{ThemeResource GridViewItemMinHeight}"/>
            <Setter Property="AllowDrop" Value="False"/>
            <Setter Property="UseSystemFocusVisuals" Value="{StaticResource UseSystemFocusVisuals}"/>
            <Setter Property="FocusVisualMargin" Value="-2"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="GridViewItem">
                        <ListViewItemPresenter x:Name="Root"
                                                   CheckBrush="{ThemeResource GridViewItemCheckBrush}" 
                                                   ContentMargin="{TemplateBinding Padding}"
                                                   CheckBoxBrush="{ThemeResource GridViewItemCheckBoxBrush}"
                                                   ContentTransitions="{TemplateBinding ContentTransitions}"
                                                   CheckMode="{ThemeResource GridViewItemCheckMode}"
                                                   DragOpacity="{ThemeResource ListViewItemDragThemeOpacity}"
                                                   DisabledOpacity="{ThemeResource ListViewItemDisabledThemeOpacity}"
                                                   DragBackground="{ThemeResource GridViewItemDragBackground}"
                                                   DragForeground="{ThemeResource GridViewItemDragForeground}"
                                                   FocusBorderBrush="{ThemeResource GridViewItemFocusBorderBrush}"
                                                   FocusVisualMargin="{TemplateBinding FocusVisualMargin}"
                                                   FocusSecondaryBorderBrush="{ThemeResource GridViewItemFocusSecondaryBorderBrush}"
                                                   HorizontalContentAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                   Control.IsTemplateFocusTarget="True"
                                                   PressedBackground="{ThemeResource GridViewItemBackgroundPressed}"
                                                   PlaceholderBackground="Transparent"
                                                   PointerOverForeground="{ThemeResource GridViewItemForegroundPointerOver}"
                                                   PointerOverBackground="Transparent"
                                                   RevealBorderThickness="{ThemeResource GridViewItemRevealBorderThemeThickness}"
                                                   ReorderHintOffset="{ThemeResource GridViewItemReorderHintThemeOffset}"
                                                   RevealBorderBrush="{ThemeResource GridViewItemRevealBorderBrush}"
                                                   RevealBackground="Transparent"
                                                   SelectedForeground="{ThemeResource GridViewItemForegroundSelected}"
                                                   SelectionCheckMarkVisualEnabled="{ThemeResource GridViewItemSelectionCheckMarkVisualEnabled}"
                                                   SelectedBackground="Transparent"
                                                   SelectedPressedBackground="Transparent" SelectedPointerOverBackground="Transparent"
                                                   VerticalContentAlignment="{TemplateBinding VerticalContentAlignment}">
                            <VisualStateManager.VisualStateGroups>
                                <VisualStateGroup x:Name="CommonStates">
                                    <VisualState x:Name="Normal"/>
                                    <VisualState x:Name="Selected"/>
                                    <VisualState x:Name="PointerOver">
                                        <VisualState.Setters>
                                            <Setter Target="Root.(RevealBrush.State)" Value="PointerOver"/>
                                        </VisualState.Setters>
                                    </VisualState>
                                    <VisualState x:Name="PointerOverSelected">
                                        <VisualState.Setters>
                                            <Setter Target="Root.(RevealBrush.State)" Value="PointerOver"/>
                                        </VisualState.Setters>
                                    </VisualState>
                                    <VisualState x:Name="PointerOverPressed">
                                        <VisualState.Setters>
                                            <Setter Target="Root.(RevealBrush.State)" Value="Pressed"/>
                                        </VisualState.Setters>
                                    </VisualState>
                                    <VisualState x:Name="Pressed">
                                        <VisualState.Setters>
                                            <Setter Target="Root.(RevealBrush.State)" Value="Pressed"/>
                                        </VisualState.Setters>
                                    </VisualState>
                                    <VisualState x:Name="PressedSelected">
                                        <VisualState.Setters>
                                            <Setter Target="Root.(RevealBrush.State)" Value="Pressed"/>
                                        </VisualState.Setters>
                                    </VisualState>
                                </VisualStateGroup>
                                <VisualStateGroup x:Name="DisabledStates">
                                    <VisualState x:Name="Enabled"/>
                                    <VisualState x:Name="Disabled">
                                        <VisualState.Setters>
                                            <Setter Target="Root.RevealBorderThickness" Value="0"/>
                                        </VisualState.Setters>
                                    </VisualState>
                                </VisualStateGroup>
                            </VisualStateManager.VisualStateGroups>
                        </ListViewItemPresenter>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Page.Resources>
    <Grid Background="{StaticResource CustomAcrylicBackgroundBrush}">
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition Width="auto"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <ScrollViewer VerticalScrollBarVisibility="Hidden"
                      ViewChanged="TimeLineScrollViewer_ViewChanged"
                      x:Name="TimeLineScrollViewer"
                      Margin="0,0,0,0" Padding="0,60,0,0" Style="{StaticResource LeptonViewScrollStyle}">
            <!--VerticalScrollBarVisibility="Hidden"-->
            <StackPanel Padding="0,60,0,0">
                <StackPanel Orientation="Horizontal">
                    <TextBlock Text="现在" FontSize="20" Visibility="Collapsed" FontWeight="Bold" Margin="25,20,0,0"></TextBlock>
                </StackPanel>

                <GridView x:Name="TabsGridView"
                          x:FieldModifier="public"
                          Style="{StaticResource GridViewStyle}" 
                          ItemContainerStyle="{StaticResource TaskGridViewItemStyle}"
                          Loaded="{x:Bind ViewModel.TabsAdaptiveGridView_Loaded}"
                          AllowDrop="True"
                          CanDragItems="True"
                          CanReorderItems="True"
                          DragItemsStarting="{x:Bind ViewModel.TabsAdaptiveGridView_DragItemsStarting}"
                          DropCompleted="{x:Bind ViewModel.TabsAdaptiveGridView_DropCompleted}"
                          ItemClick="{x:Bind ViewModel.TabsAdaptiveGridView_ItemClick}"
                          IsItemClickEnabled="True"
                          ItemsSource="{x:Bind ViewModel.TabGridViewItems,Mode=TwoWay}"
                          SelectedItem="0"
                          HorizontalAlignment="Left"
                          Margin="20,0,0,0" Padding="0,0,0,0">
                    <GridView.RenderTransform>
                        <CompositeTransform x:Name="TabsGridViewTransform" x:FieldModifier="public"></CompositeTransform>
                    </GridView.RenderTransform>
                    <GridView.ItemTemplate>
                        <DataTemplate x:DataType="viewmodels:TabGridViewItemViewModel">
                            <Grid Width="{x:Bind Width,Mode=OneWay}" Height="{x:Bind Height,Mode=OneWay}" Margin="0,0,0,10" VerticalAlignment="Center"
                                  Background="White"
                                  CornerRadius="8"><!--PointerPressed="{x:Bind Grid_PointerPressed}"-->
                                <Grid VerticalAlignment="Top">
                                    <StackPanel VerticalAlignment="Bottom" Orientation="Horizontal"
                                                HorizontalAlignment="Left" Margin="0,0,0,10">
                                        <Image Source="{x:Bind Avastar, Mode=OneWay}" Width="20" Height="20" Stretch="UniformToFill" Margin="10,0,10,0"></Image>
                                    </StackPanel>
                                    <TextBlock Text="{x:Bind Title,Mode=OneWay}"
                                               Margin="0,5,0,0"
                                               HorizontalAlignment="Center"
                                               Foreground="Black"
                                               TextAlignment="Center"></TextBlock>
                                    <Button x:Name="TabCloseButton"
                                                Foreground="#FF000000"
                                                Background="Transparent"
                                                VerticalAlignment="Bottom" HorizontalAlignment="Right"
                                                Margin="0,0,5,0"
                                                Visibility="Visible" Click="{x:Bind TabAdaptiveGridViewCloseButton_Click}">
                                        <FontIcon FontFamily="Segoe MDL2 Assets"
                                                      FontSize="12"
                                                      Glyph="&#xE894;" Margin="0,0,0,9"></FontIcon>
                                    </Button>
                                </Grid>
                                <Grid Margin="0,30,0,10" VerticalAlignment="Bottom">
                                    <Grid
                                          Background="#FFFFFFFF"
                                          VerticalAlignment="Center"
                                          CornerRadius="10"
                                          PointerEntered="{x:Bind AdaptiveGridView_PointerEntered}"
                                          PointerExited="{x:Bind AdaptiveGridView_PointerExited}">
                                        <Image HorizontalAlignment="Center" x:Name="TabsAdaptiveGridViewImage"
                                               Source="{x:Bind CapturedImage,Mode=OneWay}" Stretch="UniformToFill" Margin="0,0,0,0"></Image>
                                        <TextBlock Text="{x:Bind Id,Mode=OneWay}" Visibility="Collapsed"></TextBlock>
                                    </Grid>
                                </Grid>
                            </Grid>
                        </DataTemplate>
                    </GridView.ItemTemplate>
                </GridView>
            </StackPanel>
        </ScrollViewer>
        <Grid x:Name="TimeLineSliderGrid"  Margin="0,100,76,30" Grid.Row="1" Visibility="Collapsed">
            <TextBlock Text="当前" TextAlignment="Right" FontSize="14" HorizontalAlignment="Right" VerticalAlignment="Top" Margin="0,32,0,0"></TextBlock>
            <TextBlock x:Name="GlobalTimeLineLastTimeText" Text="今天早些时候" TextWrapping="WrapWholeWords" TextAlignment="Right" FontSize="14" HorizontalAlignment="Right" Grid.RowSpan="3" Grid.ColumnSpan="3" VerticalAlignment="Bottom"></TextBlock>
            <Slider x:Name="TimeLineSlider" Style="{StaticResource SliderStyle}" IsDirectionReversed="True"
                    Background="Black" Foreground="Black"
                    Orientation="Vertical" HorizontalAlignment="Right" Margin="0, 0, -50, 0"></Slider>
        </Grid>
        <Grid x:Name="OverViewHeader" x:FieldModifier="public" Opacity="0"  Background="#BBEEEEEE" VerticalAlignment="Top"
              Height="90">
            <Grid.Resources>
                <Storyboard x:Name="OverViewHeaderFadeIn"
                            x:FieldModifier="public"
                    Duration="0:0:1"
                    AutoReverse="True"
                    RepeatBehavior="Forever">
                    <DoubleAnimation
                        Storyboard.TargetName="OverViewHeader"
                        Storyboard.TargetProperty="Opacity"
                        From="0"
                        To="1"
                        BeginTime="0:0:1.5"
                        Duration="0:0:1.5"
                        >
                        <DoubleAnimation.EasingFunction>
                            <CircleEase EasingMode="EaseIn"></CircleEase>
                            <!--缓动函数-->
                        </DoubleAnimation.EasingFunction>
                    </DoubleAnimation>
                </Storyboard>
                <Storyboard x:Name="OverViewHeaderFadeOut"
                            x:FieldModifier="public"
                            Duration="0:0:1"
                            AutoReverse="True"
                            RepeatBehavior="Forever">
                    <DoubleAnimation
                        Storyboard.TargetName="OverViewHeader"
                        Storyboard.TargetProperty="Opacity"
                        From="1"
                        To="0"
                        BeginTime="0:0:1.5"
                        Duration="0:0:1.5"
                        >
                        <DoubleAnimation.EasingFunction>
                            <CircleEase EasingMode="EaseOut"></CircleEase>
                            <!--缓动函数-->
                        </DoubleAnimation.EasingFunction>
                    </DoubleAnimation>
                </Storyboard>
            </Grid.Resources>
            <Grid.RenderTransform>
                <CompositeTransform x:Name="OverViewHeaderTransform" x:FieldModifier="public"></CompositeTransform>
            </Grid.RenderTransform>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="auto"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <TextBlock Text="时间线" FontWeight="Bold"
                       FontSize="20" VerticalAlignment="Top"
                       Margin="30,10,0,0"></TextBlock>
            <TextBox Width="250" VerticalAlignment="Bottom"
                     Margin="23,0,0,10" CornerRadius="6"
                     BorderThickness="0"
                     Background="#FFCCCCCC"
                     PlaceholderText="在时间线中搜索标签页"
                     ></TextBox>
            <StackPanel Orientation="Horizontal" Grid.Column="2"
                        VerticalAlignment="Bottom"
                        Margin="0,0,10,10">
                <Button Content="无痕浏览" Background="Transparent"
                        Foreground="Black"
                        Margin="10 0"></Button>
                <Button Background="Transparent"
                        Margin="10 0">
                    <FontIcon FontFamily="Segoe MDL2 Assets"
                              Foreground="Black" VerticalAlignment="Center"
                              FontSize="14"
                              Glyph="&#xE710;"></FontIcon>
                </Button>
                <Button Content="完成"
                        Background="Transparent"
                        Foreground="Black"
                        Margin="10 0"></Button>
            </StackPanel>
        </Grid>
    </Grid>
</Page>
